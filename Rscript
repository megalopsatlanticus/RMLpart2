library(e1071)
srd1 <- read.csv("srd1.csv", header=FALSE)
srd2 <- read.csv("srd2.csv", header=FALSE)
y1 <- rep(0,1000)
y2 <- rep(1,1000)
y <- factor(c(y1,y2))
x <- rbind(srd1,srd2)
model <- svm(x,y,type="C-classification")
p <-predict(model,x)
plot(array(p))
f = plot(srd1, col="red")
points(srd2, col="blue")
